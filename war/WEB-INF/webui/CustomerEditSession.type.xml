<meta:Type
  xmlns:meta="class:/spiralcraft/data/types/meta/"
  xmlns:query="class:/spiralcraft/data/query/"
  xmlns:example="class:/spiralcraft/data/test/example/"
  >
  <fields>
    <meta:QueryField>
      <name>allCustomers
      </name>
      <query>
        <query:Scan>
          <type><example:Customer.type/>
          </type>
        </query:Scan>
      </query>
    </meta:QueryField>
    
    <meta:Field>
      <name>selectedCustomer
      </name>
      
      <type><example:Customer.type/>
      </type>
    </meta:Field>

    <meta:Field>
      <name>selectedCustomerId
      </name>
      
      <type><Integer.type/>
      </type>
    </meta:Field>
    
    <meta:Field>
      <name>selectedCustomerIdList
      </name>
      
      <type><Integer.array.type/>
      </type>
    </meta:Field>
    
    <meta:Field>
      <name>userAgent
      </name>
      
      <type><String.type/>
      </type>
    </meta:Field>
    
    <meta:Field>
      <name>elapsedSessionTime
      </name>
      
      <type><Long.type/>
      </type>
    </meta:Field>
    
  </fields>
</meta:Type>