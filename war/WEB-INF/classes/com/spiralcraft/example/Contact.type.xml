<?xml version="1.0" encoding="UTF-8"?>

<meta:Type
  xmlns:meta="class:/spiralcraft/data/types/meta/"
  xmlns:example="class:/spiralcraft/data/test/example"
  xmlns:j="class:/java/lang/"
  >
 

  <fields>
    <meta:SequenceField>

      <name>id
      </name>

      <type><Integer.type/>
      </type>
          
    </meta:SequenceField>

    <meta:Field>
      
      <name>displayName
      </name>
      
      <type>
        <String.type>

          <maxLength>128
          </maxLength>
        
        </String.type>
      </type>

    </meta:Field>

    
    <meta:Field>

      <name>address
      </name>

      <type>
        <example:Address.type/>
      </type>

    </meta:Field>
    
    <meta:Field>
      <name>phone
      </name>
      
      <type>
        <example:Phone.type/>
      </type>
    </meta:Field>
        
    <meta:Field>
        
      <name>active
      </name>
        	
      <type><Boolean.type/>
      </type>
        	
    </meta:Field>
        
    <meta:Field>
        
      <name>lastContact
      </name>
                
      <type><Instant.type/>
      </type>
                
    </meta:Field>
            
    <meta:Field>

      <name>paymentTerms
      </name>

      <type>
        <String.type>
          	
          <maxLength>32
          </maxLength>
              
        </String.type>
      </type>

    </meta:Field>
        
    <meta:Field>
        
      <name>score
      </name>
        
      <type>
        <Float.type/>
      </type>
    </meta:Field>
    
    <meta:CalculatedField>
        
      <name>computedScore
      </name>
        
      <type>
        <Float.type/>
      </type>
      
      <expression>score*10.0F
      </expression>
    </meta:CalculatedField>
    
    <meta:Field>
        
      <name>discountCodes
      </name>
        
      <type>
        <String.array.type/>
      </type>
      
    </meta:Field>    
    
    <meta:Field>
        
      <name>notes
      </name>
        
      <type>
        <String.type/>
      </type>
      
    </meta:Field>        
  </fields>
      
  <keys>
    <meta:Key>

      <fieldList>id
      </fieldList>
      		
      <primary>true
      </primary>
      		
    </meta:Key>


  </keys>

</meta:Type>