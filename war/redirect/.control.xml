<af:AutoFilter.list
  xmlns:af="class:/spiralcraft/servlet/autofilter/"
  xmlns:smtp="class:/spiralcraft/net/smtp/"
  xmlns:webui="class:/spiralcraft/servlet/webui/"
  xmlns:config="context://config/"
  xmlns:http="class:/javax/servlet/http/"
  xmlns:task="class:/spiralcraft/task/"
  >
  <af:RedirectFilter>
    <pattern>/
    </pattern>
    
    
    <redirectURL>index.webui
    </redirectURL>
    
    
  </af:RedirectFilter>

  <af:RedirectFilter>
    <debug>true
    </debug>
    
    <pattern>*
    </pattern>
    
    <prefix>commands/
    </prefix>
    
    <redirectURL>/commands/
    </redirectURL>
    
    <permanent>true
    </permanent>
  </af:RedirectFilter>
    
  
</af:AutoFilter.list>
  