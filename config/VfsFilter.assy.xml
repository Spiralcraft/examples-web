<svfs:VfsFilter
  xmlns:svfs="class:/spiralcraft/servlet/vfs/"
  xmlns:vfsc="class:/spiralcraft/vfs/context/"
  >
  <global>true
  </global>
  <fileSpace>
    <vfsc:FileSpace>
      <authorities>      
      
        <vfsc:Authority>
          <authorityName>
          </authorityName>
         
          <debugLevel>FINE
          </debugLevel>
          
          <grafts>
            <vfsc:Redirect>
              <virtualURI>uploads/
              </virtualURI>
              <newBaseURI>context://files/uploads/
              </newBaseURI>
            </vfsc:Redirect>
          </grafts>
        </vfsc:Authority>
        
        
      </authorities>
    </vfsc:FileSpace>
  </fileSpace>
</svfs:VfsFilter>
